MODULE main
    VAR x: integer;
    
    INIT x = 10;

    TRANS (next(x) = (x < 0 ? x : x + 1))
        | (next(x) = (x < 0
            ? x 
            : (x > 5 
                ? x + 1
                : (x > 2 ? x - 1 : x))))
        ;
    
