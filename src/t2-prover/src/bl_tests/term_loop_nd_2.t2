START: init;
FROM: init;
TO: loc1;

FROM: loc1;
assume(varX > 0);
TO: loc2;

FROM: loc1;
assume(varX <= 0);
TO: loc3;

FROM: loc3;
TO: loc3;

FROM: loc2;
varND := nondet() % 2;
TO: loc4;

FROM: loc4;
assume(varND <= 0 && varX >= 3);
TO: loc5;

FROM: loc5;
varX := varX + 1;
TO: loc2;

FROM: loc4;
assume(varND >= 1 || varX < 3);
varX := varX - 1;
TO: loc2;